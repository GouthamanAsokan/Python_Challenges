      "source": [
        "#Writing the function\n",
        "def solution(x):\n",
        "  a=str(x)\n",
        "\n",
        "  if (a[0]=='-'):\n",
        "    return int('-'+a[:0:-1])\n",
        "  else:\n",
        "    return int(a[::-1])"
      ],
      "execution_count": 1,
      "outputs": []
